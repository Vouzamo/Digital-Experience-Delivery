@model Web.Models.Article
    
<div class="col-md-12">
    <h2>@Model.Title</h2>
    <h3>@Model.Summary</h3>
    <div class="row">
        <div class="col-md-4">
            @Html.Partial("_Image", Model.Image)
        </div>
        <div class="col-md-8">
            @Html.Raw(Model.Body)
        </div>
    </div>
</div>